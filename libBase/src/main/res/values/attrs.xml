    <!--SuperButton start-->
    <declare-styleable name="SuperButton">
        <attr name="sGravity" format="enum">
            <enum name="center" value="0" />
            <enum name="left" value="1" />
            <enum name="right" value="2" />
            <enum name="top" value="3" />
            <enum name="bottom" value="4" />
        </attr>

        <attr name="sShapeType" format="enum">
            <enum name="rectangle" value="0" />
            <enum name="oval" value="1" />
            <enum name="line" value="2" />
            <enum name="ring" value="3" />
        </attr>

        <attr name="sSolidColor" format="color" />

        <attr name="sSelectorPressedColor" format="color" />
        <attr name="sSelectorDisableColor" format="color" />
        <attr name="sSelectorNormalColor" format="color" />

        <attr name="sCornersRadius" format="dimension" />

        <attr name="sCornersTopLeftRadius" format="dimension" />
        <attr name="sCornersTopRightRadius" format="dimension" />
        <attr name="sCornersBottomLeftRadius" format="dimension" />
        <attr name="sCornersBottomRightRadius" format="dimension" />

        <attr name="sStrokeWidth" format="dimension" />
        <attr name="sStrokeDashWidth" format="dimension" />
        <attr name="sStrokeDashGap" format="dimension" />
        <attr name="sStrokeColor" format="color" />

        <attr name="sSizeWidth" format="dimension" />
        <attr name="sSizeHeight" format="dimension" />

        <attr name="sGradientOrientation" format="enum">
            <enum name="TOP_BOTTOM" value="0" />
            <enum name="TR_BL" value="1" />
            <enum name="RIGHT_LEFT" value="2" />
            <enum name="BR_TL" value="3" />
            <enum name="BOTTOM_TOP" value="4" />
            <enum name="BL_TR" value="5" />
            <enum name="LEFT_RIGHT" value="6" />
            <enum name="TL_BR" value="7" />
        </attr>

        <attr name="sGradientAngle" format="dimension" />
        <attr name="sGradientCenterX" format="dimension" />
        <attr name="sGradientCenterY" format="dimension" />
        <attr name="sGradientGradientRadius" format="dimension" />
        <attr name="sGradientStartColor" format="color" />
        <attr name="sGradientCenterColor" format="color" />
        <attr name="sGradientEndColor" format="color" />
        <attr name="sGradientUseLevel" format="boolean" />

        <attr name="sUseSelector" format="boolean" />

        <attr name="sGradientType" format="enum">
            <enum name="linear" value="0" />
            <enum name="radial" value="1" />
            <enum name="sweep" value="2" />
        </attr>
    </declare-styleable>
    <!--SuperButton end-->

    <!--SCircleImageView start-->
    <declare-styleable name="SCircleImageView">
        <attr name="civ_border_width" format="dimension" />
        <attr name="civ_border_color" format="color" />
        <attr name="civ_border_overlay" format="boolean" />
        <attr name="civ_fill_color" format="color" />
        <attr name="civ_circle_background_color" format="color" />
    </declare-styleable>
    <!--SCircleImageView end-->

    <!--SlidingTabLayout start-->
    <declare-styleable name="SlidingTabLayout">
        <!-- indicator -->
        <attr name="tl_indicator_color"/>
        <attr name="tl_indicator_height"/>
        <attr name="tl_indicator_width"/>
        <attr name="tl_indicator_margin_left"/>
        <attr name="tl_indicator_margin_top"/>
        <attr name="tl_indicator_margin_right"/>
        <attr name="tl_indicator_margin_bottom"/>
        <attr name="tl_indicator_corner_radius"/>
        <attr name="tl_indicator_gravity"/>
        <attr name="tl_indicator_style"/>
        <attr name="tl_indicator_width_equal_title"/>
        <!-- underline -->
        <attr name="tl_underline_color"/>
        <attr name="tl_underline_height"/>
        <attr name="tl_underline_gravity"/>
        <!-- divider -->
        <attr name="tl_divider_color"/>
        <attr name="tl_divider_width"/>
        <attr name="tl_divider_padding"/>
        <!-- tab -->
        <attr name="tl_tab_padding"/>
        <attr name="tl_tab_space_equal"/>
        <attr name="tl_tab_width"/>
        <!-- title -->
        <attr name="tl_textsize"/>
        <attr name="tl_textSelectColor"/>
        <attr name="tl_textUnselectColor"/>
        <attr name="tl_textBold"/>
        <attr name="tl_textAllCaps"/>
    </declare-styleable>
    <!--SlidingTabLayout end-->

    <!--MsgView start-->
    <declare-styleable name="MsgView">
        <!-- 圆角矩形背景色 -->
        <attr name="mv_backgroundColor" format="color"/>
        <!-- 圆角弧度,单位dp-->
        <attr name="mv_cornerRadius" format="dimension"/>
        <!-- 圆角弧度,单位dp-->
        <attr name="mv_strokeWidth" format="dimension"/>
        <!-- 圆角边框颜色-->
        <attr name="mv_strokeColor" format="color"/>
        <!-- 圆角弧度是高度一半-->
        <attr name="mv_isRadiusHalfHeight" format="boolean"/>
        <!-- 圆角矩形宽高相等,取较宽高中大值-->
        <attr name="mv_isWidthHeightEqual" format="boolean"/>
    </declare-styleable>
    <!--MsgView end-->

    <!--SwitchButton start-->
    <declare-styleable name="SwitchButton">
        <attr name="sbCheckColor" format="color" />
        <attr name="sbUnCheckColor" format="color" />
        <attr name="sbChecked" format="boolean" />
        <attr name="sbThumbColor" format="color" />
    </declare-styleable>
    <!--SwitchButton end-->

    <!--CLRoundLayout start-->
    <declare-styleable name="CLRoundLayout">
        <attr name="rRadii" format="integer" />
        <attr name="rRadii_top_left" format="dimension" />
        <attr name="rRadii_top_right" format="dimension" />
        <attr name="rRadii_bottom_left" format="dimension" />
        <attr name="rRadii_bottom_right" format="dimension" />
    </declare-styleable>
    <!--CLRoundLayout end-->
</resources>